{
  "name": "dataspace",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "postinstall": "npx prisma generate",
    "production:entry": "prisma migrate deploy && npx prisma db seed && node .",
    "production:build": "docker compose -f docker-compose.prod.yml build",
    "production:up": "docker compose -f docker-compose.prod.yml up",
    "production:down": "docker compose -f docker-compose.prod.yml down"
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "^5.4.0",
    "@sveltejs/enhanced-img": "^0.8.2",
    "@sveltejs/kit": "^2.22.0",
    "@sveltejs/vite-plugin-svelte": "^6.0.0",
    "@types/node": "^24.5.2",
    "prisma": "^6.16.3",
    "sass": "^1.93.1",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "typescript": "^5.0.0",
    "vite": "^7.2.6"
  },
  "dependencies": {
    "@ds4sscc/node-odrl": "^1.1.0",
    "@prisma/client": "^6.16.3",
    "@sourceregistry/node-env": "^1.0.0",
    "@sourceregistry/node-jwt": "^1.2.1",
    "@sourceregistry/node-opa": "^1.0.1",
    "@sourceregistry/svelte-service-manager": "^0.0.40",
    "bootstrap-icons": "^1.13.1",
    "echarts": "^6.0.0",
    "marked": "^16.4.1",
    "monaco-editor": "^0.55.1"
  }
}
